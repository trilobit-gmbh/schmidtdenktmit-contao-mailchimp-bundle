{
    "name": "trilobit-gmbh/schmidtdenktmit-contao-mailchimp-bundle",
    "type": "contao-bundle",
    "description": "Contao 4 mailchimp bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "trilobit GmbH",
            "homepage": "https://github.com/trilobit-gmbh"
        }
    ],
    "require": {
        "php": "^7.0",
        "contao/core-bundle": "~4.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Schmidtdenktmit\\MailchimpBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Schmidtdenktmit\\MailchimpBundle\\ContaoManager\\Plugin"
    }
}
